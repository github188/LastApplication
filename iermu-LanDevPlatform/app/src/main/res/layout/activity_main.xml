
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="wrap_content" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity">
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="开始平扫"
        android:id="@+id/button1"
        android:layout_alignRight="@+id/button7"
        android:layout_alignEnd="@+id/button7"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="停止平扫"
        android:id="@+id/button2"
        android:layout_alignTop="@+id/button1"
        android:layout_toRightOf="@+id/button1"
        android:layout_marginLeft="2dp"/>
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="上传预置位"
        android:id="@+id/button3"
        android:layout_alignTop="@+id/button2"
        android:layout_toRightOf="@+id/button2"
        android:layout_marginLeft="2dp"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="执行预置位"
        android:id="@+id/button4"
        android:layout_below="@+id/button1"
        android:layout_toLeftOf="@+id/button2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="云台位移"
        android:id="@+id/button5"
        android:layout_alignTop="@+id/button4"
        android:layout_toRightOf="@+id/button4"
        android:layout_marginLeft="2dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="是否平扫"
        android:id="@+id/button6"
        android:layout_alignTop="@+id/button5"
        android:layout_toRightOf="@+id/button5"
        android:layout_alignRight="@+id/button3"
        android:layout_marginLeft="2dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="是否支持位移"
        android:id="@+id/button7"
        android:layout_below="@+id/button4" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="是否云台"
        android:id="@+id/button8"
        android:layout_alignTop="@+id/button7"
        android:layout_toRightOf="@+id/button7"
        android:layout_marginLeft="2dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="卡录列表"
        android:id="@+id/button9"
        android:layout_alignTop="@+id/button8"
        android:layout_toRightOf="@+id/button8"
        android:layout_alignRight="@+id/button3"
        android:layout_marginLeft="2dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="卡录播放路径"
        android:id="@+id/button10"
        android:layout_below="@+id/button7"
        android:layout_alignParentLeft="true"
        android:layout_alignRight="@+id/button7"/>
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="获取目录"
        android:id="@+id/button11"
        android:layout_alignTop="@+id/button10"
        android:layout_toRightOf="@+id/button10"
        android:layout_marginLeft="2dp"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="lan播放"
        android:id="@+id/button12"
        android:layout_alignTop="@+id/button10"
        android:layout_toRightOf="@+id/button11"
        android:layout_alignRight="@+id/button9"
        android:layout_marginLeft="2dp"/>
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="停止lan播放"
        android:id="@+id/button13"
        android:layout_below="@+id/button10"
        android:layout_alignParentLeft="true"
        android:layout_alignRight="@id/button1"/>
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="nas参数"
        android:id="@+id/button14"
        android:layout_alignTop="@+id/button13"
        android:layout_toRightOf="@+id/button13"
        android:layout_alignRight="@+id/button2"
        android:layout_marginLeft="2dp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="参数"
        android:id="@+id/textView1"
        android:layout_below="@id/button13"
        android:layout_alignParentLeft="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/editText1"
        android:layout_below="@id/textView1"
        android:layout_alignParentLeft="true"
        android:layout_alignRight="@+id/button2" />
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/editText2"
        android:layout_below="@id/editText1"
        android:layout_alignParentLeft="true"
        android:layout_alignRight="@+id/button2" />

    <!--<EditText-->
        <!--android:layout_width="wrap_content"-->
        <!--android:layout_height="wrap_content"-->
        <!--android:id="@+id/editText3"-->
        <!--android:layout_below="@id/editText2"-->
        <!--android:layout_alignParentLeft="true"-->
        <!--android:layout_alignRight="@+id/button2" />-->

    <!--<EditText-->
        <!--android:layout_width="wrap_content"-->
        <!--android:layout_height="wrap_content"-->
        <!--android:id="@+id/editText4"-->
        <!--android:layout_below="@id/editText3"-->
        <!--android:layout_alignParentLeft="true"-->
        <!--android:layout_alignRight="@+id/button2" />-->

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="返回值"
        android:id="@+id/textView"
        android:layout_below="@id/editText2"
        android:layout_alignParentLeft="true"
        android:layout_alignRight="@+id/button15" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="卡录信息"
        android:id="@+id/button15"
        android:layout_alignTop="@+id/button13"
        android:layout_toRightOf="@+id/button14"
        android:layout_alignRight="@+id/button12"
        android:layout_marginLeft="2dp"/>


</RelativeLayout>

